package test {
	
	native testFail(string msg)
	native testSuccess()

	
	class Bla {
		int i = 13
		int j = 27
		static int k = 36
		
		construct(int j) {
			this.j = j
		}
		
		function getI() returns int {
			return i
		}
		
		function getJ() returns int {
			return j
		}
		
		function getK() returns int {
			return k
		}
	}
	

	init {
		Bla b = new Bla(14)
		if b.getI() != 13 {
			testFail("i")
		} else if b.getJ() != 14 {
			testFail("j")
		} else if b.getK() != 36 {
			testFail("k")
		} else {
			testSuccess()
		}
	} 
	
	
}
