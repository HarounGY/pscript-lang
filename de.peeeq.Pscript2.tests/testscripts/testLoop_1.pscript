package test {
	native type Int = integer
	native type Bool = boolean
	native type String = string
	
	function testFail(msg:String) {
		buildin testFail(msg)
	}
	
	function testSuccess() {
		buildin testSuccess()
	}
	
	
	function runTest() {
		var i = 0
		while (i < 10) {
			i = i + 1
		}
		if (i == 10) {
			testSuccess()
		} else {
			testFail("defect in whileLoop")
		}
	}
	
	
}